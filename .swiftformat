# file options

--exclude Tests/XCTestManifests.swift,Tests/BadConfig,Snapshots,Build,\*.pb.swift,g/Packages/coredata/gen,g/Packages/proto/gen

# format options

--allman false
--binarygrouping 4,8
--commas always
--comments indent
--decimalgrouping 3,6
--elseposition same-line
--empty void
--exponentcase lowercase
--exponentgrouping disabled
--fractiongrouping disabled
--generictypes true
--header ignore
--hexgrouping 4,8
--hexliteralcase uppercase
--ifdef indent
--indent "tab"
--indentcase false
--importgrouping testable-bottom
--linebreaks lf
--maxwidth none
--octalgrouping 4,8
--operatorfunc spaced
--patternlet hoist
--ranges spaced
--self insert
--semicolons inline
--stripunusedargs always
--swiftversion 5.7
--trimwhitespace always
--wraparguments preserve
--wrapcollections preserve
--redundanttype explicit
--acronyms "URL,URLSession,JSON,HTTP,HTTPS,WSS,API,ID,UUID,MQTT,IP,IPv4,IOTCore,AWS,PNG,JWT"

# rules

--enable acronyms
--disable redundantReturn
